{
  "version": 2,
  "buildCommand": "npm install",
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "NODE_ENV": "production"
  },
  "functions": {
    "server.js": {
      "runtime": "nodejs18.x",
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
